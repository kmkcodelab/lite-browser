{
  "expo": {
    "name": "Lite Browser",
    "slug": "lite-browser",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "backgroundColor": "#000000"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.litebrowser.app",
      "permissions": ["INTERNET"]
    },
    "plugins": [
      "expo-screen-orientation"
    ]
  }
}
